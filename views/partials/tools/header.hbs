<div class="header-wrapper header-wrapper--fixed {{headerAdditionalClasses}}" data-ms-theme="tools">
    {{#if flash.enabled}} {{> components/flash}} {{/if}}
    {{> components/cookieDisclaimer}}
    <header role="banner" class="header" itemtype="http://schema.org/Store" itemid="#this" itemscope="" class="nav-tools">
        <div class="nav-tools-header__top header--custom-background">
            
            <div class="nav-tools-header__top-bound">
                <ul class="menu-options">
                    <li class="options nav-list__item">
                        <a class="productos-link">PRODUCTOS</a>
                        <div class="doble-menu">
                            <ul class="lista-submenu-doble">
                                <li class="items-submenu"><a href="/aceites"><img src="https://iili.io/JJyjlGp.png" class="icon-list" alt="">ACEITES</a></li>
                                <li class="items-submenu"><a href="/electricidad"><img src="https://iili.io/JJpVuwl.png" class="icon-list" alt="">ELECTRICIDAD</a></li>
                                <li class="items-submenu"><a href="/alternadores"><img src="https://iili.io/JJpMeTl.png" class="icon-list" alt="">ALTERNADORES</a></li>
                                <li class="items-submenu"><a href="/arranques"><img src="https://iili.io/JJpMvQS.png" class="icon-list" alt="">ARRANQUES</a></li>
                                <li class="items-submenu"><a href="/baterias"><img src="https://iili.io/JJpVOcg.png" class="icon-list" alt="">BATERÍAS</a></li>
                                <li class="items-submenu"><a href="/bujias"><img src="https://iili.io/JJpWCxV.png" class="icon-list" alt="">BUJÍAS</a></li>
                                <li class="items-submenu"><a href="/encendido"><img src="https://iili.io/JJpV08b.png" class="icon-list" alt="">ENCENDIDO</a></li>
                                <li class="items-submenu"><a href="/filtros"><img src="https://iili.io/JJpWTUg.png" class="icon-list" alt="">FILTROS</a></li>
                                <li class="items-submenu"><a href="/frenos"><img src="https://iili.io/JJpX0Tx.png" class="icon-list" alt="">FRENOS</a></li>
                                <li class="items-submenu"><a href="/suspension-direccion"><img src="https://iili.io/JJpj4mG.png" class="icon-list" alt="">SUSPENSIÓN Y DIRECCIÓN</a></li>
                            </ul>
                        </div>
                    </li>
                    <li class="options">
                        <a class="nuestra-empresa-link" href="/nosotros">NUESTRA EMPRESA</a>
                    </li>
                    <li class="options">
                        <a class="sucursal-link" href="/sucursales">SUCURSALES</a>
                    </li>
                    <li class="options">
                        <a class="consultas-link" href="/p/contacto">CONSULTAS</a>
                    </li>
                </ul>
                <!-- 
                <a href="/" id="logo-wrapper" class="header-logo">
                    <img id="image-logo" src="https://iili.io/JJzgHAu.png" class="header-logo-img" alt="Grupo Rosales" />
                </a>
                -->
                <a href="/" id="logo-wrapper" class="header-logo">
                    {{#if logo}}
                    <img id="image-logo" class="header-logo-img" src="https://iili.io/JJzgHAu.png" alt="{{name}}" />
                    {{else}}
                    <span class="header--custom-text">
                        {{name}}
                    </span>
                    {{/if}}
                </a>
                <!-- Search box -->
                <div class="nav-tools-header__top-bound-separation">
                    <div class="top-separation">
                        <label class="nav-tools-header__searchbox-mock" for="search-box-toggle"> </label>
                        <input type="checkbox" name="search-box-toggle" id="search-box-toggle" />
                        <div class="nav-tools-header__searchbox {{hasInfoClass}}">
                            {{#if headerMessageEnabled}}
                            <div class="header__searchbox-info header--custom-text">
                                {{{headerMessage}}}
                            </div>
                            
                            <!-- /.header__searchbox-info header--custom-text -->
                            {{/if}}
                            <form class="search-box-container" id="search-form" action="{{home_url}}">
                                <div class="close"></div>
                                <input type="text" id="search-input" name="search-box-tools" placeholder="{{i18n.placeholder}}" />
                                <button type="submit" class="search-button">
                                    {{> tools/views/partials/search}}
                                </button>
                                <!-- /.search-button -->
                            </form>
                        </div>
                        <!-- Icons contact/cart -->
                        <nav class="nav-tools-header__icons">
                            {{#if cart.href}}
                            <a href="{{cart.href}}" title="Carrito" class="cart-button" id="nav-cart">
                                {{> tools/views/partials/cart-badge}}
                                {{> tools/views/partials/cart}}
                            </a>
                            {{/if}}
                            <!-- Required for menu-responsive widget -->
                            <input type="checkbox" id="mobile-categories-menu-switch" name="nav-tools-header__menu-switch" />
                            <label for="mobile-categories-menu-switch" id="hamburguer-menu">
                                <span class="nav-header__hamburger"></span>
                                <span class="nav-header__hamburger"></span>
                                <span class="nav-header__hamburger"></span>
                            </label>
                            <!-- Mobile categories menu has to be a sibling of switcher-->
                            <ul class="categorias-mobile nav-list nav-list--vertical header--custom-background" itemtype="http://schema.org/SiteNavigationElement" itemscope>

                                <li><a href="/nuestros-productos">Productos</a><img src="https://cdn-icons-png.flaticon.com/512/8442/8442675.png" class="flecha-desplegable" alt="flecha-desplegable"></li>
                                <ul class="subcategorias invisible">
                                    <li><img src="https://iili.io/JJyjlGp.png" class="icon-list" alt=""><a href="/aceites">Aceites</a></li>
                                    <li><img src="https://iili.io/JJpVuwl.png" class="icon-list" alt=""><a href="/electricidad">Electricidad</a></li>
                                    <li><img src="https://iili.io/JJpMeTl.png" class="icon-list" alt=""><a href="/alternadores">Alternadores</a></li>
                                    <li><img src="https://iili.io/JJpMvQS.png" class="icon-list" alt=""><a href="/arranques">Arranques</a></li>
                                    <li><img src="https://iili.io/JJpVOcg.png" class="icon-list" alt=""><a href="/baterias">Baterías</a></li>
                                    <li><img src="https://iili.io/JJpWCxV.png" class="icon-list" alt=""><a href="/bujias">Bujías</a></li>
                                    <li><img src="https://iili.io/JJpV08b.png" class="icon-list" alt=""><a href="/encendido">Encendido</a></li>
                                    <li><img src="https://iili.io/JJpWTUg.png" class="icon-list" alt=""><a href="/filtros">Filtros</a></li>
                                    <li><img src="https://iili.io/JJpX0Tx.png" class="icon-list" alt=""><a href="/frenos">Frenos</a></li>
                                    <li><img src="https://iili.io/JJpj4mG.png" class="icon-list" alt=""><a href="/suspension-direccion">Suspensión y dirección</a></li>
                                </ul>
                                <hr>
                                <li><a href="/nosotros">Nuestra Empresa</a></li>
                                <hr>
                                <li><a href="/sucursales">Sucursales</a></li>
                                <hr>
                                <li><a href="/p/contacto">Consultas</a></li>
                                <hr>
                                <li class="nav-tools-list--vertical__category header--custom-text how-buy__button" data-js="how-buy__button" itemprop="name" tools-custom-data-emitter="how-buy__button">
                                    <a href="{{linkToHowBuy.href}}">{{linkToHowBuy.label}}</a>
                                </li>
                            </ul>
                        </nav>
                    </div>
                    <!-- Desktop navigation menu -->
                    
                </div>
            </div>
        </div>
    </header>
</div>